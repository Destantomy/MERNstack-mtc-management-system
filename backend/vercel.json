

{
  "version": 2,
  "builds": [
    {
      "src": "sources/server.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    { "src": "/api/datalog/:id", "dest": "sources/routes/routes.js" },
    { "src": "/api/datalog/(.*)", "dest": "sources/routes/routes.js" },
    { "src": "/(.*)", "dest": "sources/server.js" }
  ]
}
